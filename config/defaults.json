{
  "esclient": {
    "apiVersion": "1.1",
    "keepAlive": true,
    "requestTimeout": "120000",
    "hosts": [{
      "env": "development",
      "protocol": "http",
      "host": "localhost",
      "port": 9200
    }],
    "log": [{
      "type": "stdio",
      "level": [ "error", "warning" ]
    }]
  },
  "imports": {
    "geonames": {
      "datapath": "./data"
    },
    "quattroshapes": {
      "datapath": "/mnt/pelias/quattroshapes"
    },
    "openstreetmap": {
      "datapath": "/mnt/pelias/openstreetmap",
      "leveldbpath": "/tmp",
      "import": [{
        "type": { "node": "osmnode", "way": "osmway" },
        "filename": "planet.osm.pbf"
      }]
    }
  }
}